{
    // Settings
    "passfail": false,
    "maxerr": 100,

    // Predefined globals
    "browser": true,
    "jquery": true,
    "predef": ["require", "define", "module"],

    // Development
    "debug": false,
    "devel": true,
    
    // ECMAScript 5.
    "strict": false,
    "globalstrict": false,
    
    // The Good Parts
    "asi": false,
    "laxbreak": false,
    "bitwise": false,
    "boss": false,
    "curly": true,
    "eqeqeq": true,
    "eqnull": false,
    "evil": false,
    "expr": false,
    "forin": false,
    "immed": true,
    "latedef": true,
    "loopfunc": false,
    "noarg": true,
    "scripturl": true,
    "shadow": false,
    "supernew": false,
    "undef": true,

    // Personal styling preferences
    "newcap": true,
    "noempty": true,
    "nonew": true,
    "nomen": false,
    "onevar": true,
    "plusplus": true,
    "sub": false,
    "trailing": true,
    "white": false,
    "indent": 4
}
